msgid "Waitress"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "James"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "Foreign Elf"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "Corporate Man"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "Tech Vendor"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "Corporate Woman"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "Tailor"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "Stressed Suit"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
msgid "Food Stall Worker"
msgstr ""

#: /data/scenes/a2_lodge_s1.srt:CharacterInstance:char_name
