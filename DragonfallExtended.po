msgid "Deck Upgrade"
msgstr ""

#: /data/chars/blitz_focus_3_a.cvf:CharacterVariant:short_desc
msgid ""
"Gain Gear:\n"
"Cyberdeck"
msgstr ""

#: /data/chars/blitz_focus_3_a.cvf:CharacterVariant:long_desc
msgid ""
"Blitz gains a Fairlight Excalibur cyberdeck, replacing his old VirtuaX deck."
msgstr ""

#: /data/chars/blitz_focus_3_b.cvf:CharacterVariant:name
msgid "Cyberware Installation"
msgstr ""

#: /data/chars/blitz_focus_3_b.cvf:CharacterVariant:short_desc
msgid ""
"Gain Cyberware:\n"
"Hydraulic Jack"
